{
  "name": "paperback-mdchecker",
  "version": "1.0.0",
  "description": "A chapter update check managing server",
  "main": "index.js",
  "repository": "git@github.com:henderea/paperback-mdchecker.git",
  "author": "Eric Henderson <henderea@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint bin lib ts-types",
    "lint:fix": "eslint --fix bin lib ts-types",
    "start": "ts-node ./bin/index.ts",
    "clean": "rm -rf build",
    "build": "yarn clean && yarn build:app && yarn build:update-check",
    "build:app": "./scripts/ncc-build-release.sh app",
    "build:update-check": "./scripts/ncc-build-release.sh update-check"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "got": "13.0.0",
    "http-terminator": "^3.2.0",
    "node-schedule": "^2.1.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/got": "^9.6.12",
    "@types/node": "^20.5.7",
    "@types/node-schedule": "^2.1.0",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.48.0",
    "eslint-config-henderea": "1.1.12",
    "eslint-plugin-import": "^2.28.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "eslintConfig": {
    "extends": "henderea"
  }
}
